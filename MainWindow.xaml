                        <Label Content="Mod Index Ki" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Padding="0" Margin="15,0,94,0"/>
                        <TextBox x:Name="tbModIndexKi" TextWrapping="Wrap" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Width="68" Margin="0" KeyDown="ValueIsChanged_Handler"/>
                    </StackPanel>
                </StackPanel>
            </TabItem>
            <TabItem Header="Gains" Margin="-2,0">
                <StackPanel Height="216">
                    <StackPanel Margin="0,2" Height="20" Orientation="Horizontal">
                        <Label Content="Speed Kp" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Padding="0" Margin="5,0,66,0"/>
                        <TextBox Name="tbSpeedKp" TextWrapping="Wrap" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Width="68" Margin="0" KeyDown="ValueIsChanged_Handler"/>
                        <Label Content="Speed Ki" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Padding="0" Margin="50,0,90,0"/>
                        <TextBox Name="tbSpeedKi" TextWrapping="Wrap" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Width="68" Margin="0" KeyDown="ValueIsChanged_Handler"/>
                    </StackPanel>
                    <StackPanel Margin="0,2" Height="20" Orientation="Horizontal">
                        <Label Content="Peak Torque, Nm" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Padding="0" Margin="5,0,26,0"/>
                        <TextBox Name="tbPeakTorque" TextWrapping="Wrap" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Width="68" Margin="0" KeyDown="ValueIsChanged_Handler"/>
                        <Label Content="Max Torque, % of peak" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Padding="0" Margin="50,0,15,0"/>
                        <TextBox Name="tbMaxTorque" TextWrapping="Wrap" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Width="68" Margin="0" KeyDown="ValueIsChanged_Handler"/>
                    </StackPanel>
                    <StackPanel Margin="0,2" Height="20" Orientation="Horizontal">
                        <Label Content="Current Limit, Arms" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Padding="0" Margin="5,0,14,0"/>
                        <TextBox Name="tbCurrentLimit" TextWrapping="Wrap" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Width="68" Margin="0" KeyDown="ValueIsChanged_Handler"/>
                        <Label Content="Torque Slope, % rated/s" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Padding="0" Margin="50,0,10,0"/>
                        <TextBox x:Name="tbTorqueSlope" TextWrapping="Wrap" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Width="68" Margin="0" KeyDown="ValueIsChanged_Handler"/>
                    </StackPanel>
                </StackPanel>
            </TabItem>
        </TabControl>
        <DockPanel Margin="0,2,6,2" Height="20" HorizontalAlignment="Center" >
            <Button x:Name="btnRead" Content="Read" HorizontalAlignment="Right" HorizontalContentAlignment="Center" Margin="5,0,20,0" Click="btnRead_ClickHandler"/>
            <Button Content="Load" HorizontalAlignment="Right" HorizontalContentAlignment="Center" Margin="5,0,55,0" Click="btnLoad_ClickHandler"/>
        </DockPanel>
        <StatusBar>
            <Label Content="CAN dongle: " FontSize="10"/>
            <TextBlock Name="tbCanAdapter" FontSize="10"/>
        </StatusBar>
    </StackPanel>
</Window>
